        </activity>
        <activity
            android:name=".User"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".AboutUs" />
        <activity android:name=".Help" />
        <activity android:name=".Desp" />
        <activity android:name=".Register" />
        <activity android:name=".Change_Pass" />
